{
  "expo": {
    "name": "Family Home Portal",
    "slug": "Family-home-portal",
    "owner": "vrindakansal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor":"#ffffff",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.familycarespace.familyhomeportal",
      "buildNumber":"1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses the gallery to upload selected photos to Activity Stream."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package":"com.familycarespace.familyhomeportal",
      "versionCode":1,
      "permissions":["READ_EXTERNAL_STORAGE","CAMERA_ROLL"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
